{
    "name": "@iliad-ui/icons-editor",
    "version": "0.15.0",
    "publishConfig": {
        "access": "public"
    },
    "description": "Auto create icons components from remote figma path",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/gaoding-inc/iliad-ui.git",
        "directory": "packages/icons-ui"
    },
    "author": "",
    "homepage": "https://adobe.github.io/spectrum-web-components/components/icons-ui",
    "bugs": {
        "url": "https://github.com/adobe/spectrum-web-components/issues"
    },
    "main": "./src/index.js",
    "type": "module",
    "exports": {
        ".": "./src/index.js",
        "./src/*": "./src/*",
        "./icons/*": "./icons/*",
        "./package.json": "./package.json"
    },
    "scripts": {
        "build": "node bin/build.js",
        "build:clean": "yarn fetch && yarn build",
        "fetch": "node bin/fetchSVG.js",
        "test": "echo \"Error: run tests from root\" && exit 1"
    },
    "files": [
        "**/*.d.ts",
        "**/*.js",
        "**/*.js.map",
        "!bin/",
        "custom-elements.json",
        "!stories/",
        "!test/"
    ],
    "keywords": [
        "icon",
        "svg",
        "figma"
    ],
    "dependencies": {
        "@iliad-ui/base": "^0.12.2",
        "@iliad-ui/icon": "^0.17.2",
        "tslib": "^2.0.0"
    },
    "devDependencies": {
        "cheerio": "^1.0.0-rc.3",
        "dotenv": "^8.2.0",
        "figma-js": "^1.11.0",
        "fs-extra": "^9.0.1",
        "got": "^11.5.1",
        "p-queue": "2.4.2",
        "svgo": "^1.3.0",
        "uppercamelcase": "^3.0.0"
    },
    "types": "./src/index.d.ts",
    "customElements": "custom-elements.json",
    "gitHead": "2c03ae2437dbe18e8d274b92575c57aeddd90344",
    "maintainers": [
        {
            "name": "saiye",
            "email": "saiye@gaoding.com"
        },
        {
            "name": "yase",
            "email": "yase@gaoding.com"
        }
    ],
    "sideEffects": [
        "./src/index.js",
        "./icons/*"
    ]
}
